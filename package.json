{
  "name": "mini-async-scheduler",
  "version": "0.1.0",
  "description": "限制并发数量的任务调度器",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run build && jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "format": "prettier --write --parser typescript \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --fix .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gracehui88/mini-async-scheduler"
  },
  "keywords": [
    "mini-async-scheduler",
    "concurrency",
    "async",
    "queue"
  ],
  "author": "Grace",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gracehui88/mini-async-scheduler/issues"
  },
  "homepage": "https://github.com/gracehui88/mini-async-scheduler",
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "markdownlint": "^0.21.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{tsx,ts}": [
      "format",
      "lint"
    ]
  }
}
